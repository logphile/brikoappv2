{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/_nuxt/*", "/fonts/*", "/icons/*", "/images/*"]
  },
  "mimeTypes": { ".js": "application/javascript", ".json": "application/json" },
  "responseOverrides": {},
  "globalHeaders": {
    "cache-control": "public, max-age=0, must-revalidate"
  },
  "routes": [
    {
      "route": "/_nuxt/*",
      "headers": { "cache-control": "public, max-age=31536000, immutable" }
    }
  ]
}
