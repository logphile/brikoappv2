{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/_nuxt/*",
      "/fonts/*",
      "/icons/*",
      "/images/*"
    ]
  },
  "mimeTypes": {
    ".js": "application/javascript",
    ".json": "application/json"
  },
  "responseOverrides": {},
  "globalHeaders": {
    "cache-control": "public, max-age=0, must-revalidate"
  },
  "routes": [
    {
      "route": "/_nuxt/*",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "route": "/index.html",
      "redirect": "/",
      "statusCode": 301
    },
    {
      "route": "/how-it-works/",
      "redirect": "/how-it-works",
      "statusCode": 301
    },
    {
      "route": "/gallery/",
      "redirect": "/gallery",
      "statusCode": 301
    },
    {
      "route": "/avatar/",
      "redirect": "/avatar",
      "statusCode": 301
    },
    {
      "route": "/mosaic/",
      "redirect": "/mosaic",
      "statusCode": 301
    },
    {
      "route": "/voxel/",
      "redirect": "/voxel",
      "statusCode": 301
    },
    {
      "route": "/pricing/",
      "redirect": "/pricing",
      "statusCode": 301
    },
    {
      "route": "/terms/",
      "redirect": "/terms",
      "statusCode": 301
    },
    {
      "route": "/privacy/",
      "redirect": "/privacy",
      "statusCode": 301
    }
  ]
}
