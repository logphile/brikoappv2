{
  "routes": [
    { "route": "/login",  "serve": "/login/index.html" },
    { "route": "/login/", "serve": "/login/index.html" },
    { "route": "/login/*", "serve": "/login/index.html" }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/assets/*", "/*.js", "/*.css", "/*.ico", "/*.png", "/*.svg"]
  }
}
