{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/assets/*",
      "/_nuxt/*",
      "/fonts/*",
      "/icons/*",
      "/images/*",
      "/favicon.*",
      "/robots.txt",
      "/sitemap.xml",
      "/api/*",
      "/slider/*"
    ]
  },
  "mimeTypes": {
    ".webp": "image/webp",
    ".woff2": "font/woff2",
    ".woff": "font/woff"
  },
  "routes": [
    {
      "route": "/_nuxt/*",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "route": "/slider/*",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "route": "/index.html",
      "headers": {
        "cache-control": "no-store"
      }
    },
    { "route": "/old-path",          "redirect": "/new-path",    "statusCode": 301 },
    { "route": "/old",                "redirect": "/",            "statusCode": 301 },
    { "route": "/community-studio",   "redirect": "/gallery",     "statusCode": 301 },
    { "route": "/community-studio/*", "redirect": "/gallery",     "statusCode": 301 },
    { "route": "/photo-to-bricks",    "redirect": "/mosaic",      "statusCode": 301 },
    { "route": "/sitemap",            "redirect": "/sitemap.xml", "statusCode": 301 }
  ],
  "globalHeaders": {
    "cache-control": "no-store"
  }
}
