{
  "routes": [
    { "route": "/login",  "rewrite": "/login/index.html" },
    { "route": "/login/", "rewrite": "/login/index.html" },
    { "route": "/login/*", "rewrite": "/login/index.html" }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/_nuxt/*", "/assets/*", "/*.js", "/*.css", "/*.ico", "/*.png", "/*.svg"]
  }
}
