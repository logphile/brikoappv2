{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/assets/*",
      "/_nuxt/*",
      "/favicon.*",
      "/robots.txt",
      "/slider/*"
    ]
  },
  "responseOverrides": {
    "404": { "rewrite": "/index.html", "statusCode": 200 }
  },
  "mimeTypes": {
    ".webp": "image/webp",
    ".woff2": "font/woff2",
    ".woff": "font/woff"
  },
  "routes": [
    {
      "route": "/_nuxt/*",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "route": "/slider/*",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "route": "/index.html",
      "headers": {
        "cache-control": "no-store"
      }
    }
  ],
  "globalHeaders": {
    "cache-control": "no-store"
  }
}
